{{#each env.cloudAccounts}}

module "{{accountPrefix this}}_core" {
  source  = "pagopa-dx/azure-core-infra/azurerm"
  version = "~> 2.0"

  providers = {
    azurerm = azurerm.{{snakeCase displayName}}
  }

  environment = local.environment.{{accountPrefix this}}

  tags = local.tags
}

{{/each}}


