{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "outDir": "../typescript-generator/lib",
    "rootDir": "../typescript-generator/src",
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "bundler",
    "module": "ESNext"
  },
  "include": ["../typescript-generator/src/**/*"],
  "exclude": [
    "../typescript-generator/tsup.config.ts",
    "../typescript-generator/templates",
    "../typescript-generator/dist"
  ]
}
