{
  "name": "provider-azure",
  "version": "0.8.2",
  "private": true,
  "description": "Terraform provider for DX on Azure, to enhance developer experience",
  "nx": {
    "projectType": "library",
    "targets": {
      "format": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "{projectRoot}",
          "command": "go fmt ./...",
          "forwardAllArgs": false
        }
      },
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "{projectRoot}",
          "command": "go test ./...",
          "forwardAllArgs": false
        }
      }
    }
  }
}
